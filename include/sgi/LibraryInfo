#pragma once

class QObject;

namespace sgi {

template<typename AUTOLOADCLASS>
inline QObject * libraryInfoObject()
{
    QObject * ret;
    typedef QObject * (*pfn)();
    pfn func = (pfn)AUTOLOADCLASS::sgiLibraryProcAddress("sgi_libraryInfoObject");
    if(func)
        ret = func();
    else
        ret = nullptr;
    return ret;
}

} // namespace sgi
